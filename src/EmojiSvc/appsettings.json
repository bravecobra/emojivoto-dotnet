{
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=Emojies.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },
    "Endpoints": {
      "Http1": {
        "Url": "http://0.0.0.0:81",
        "Protocols": "Http1"
      },
      "Http": {
        "Url": "http://0.0.0.0:80",
        "Protocols": "Http2"
      }
    }
  },
  "UseTracingExporter": "default",
  "UseMetricsExporter": "default",
  "UseLogExporter": "default",
  "Seq": {
    "Uri": "http://localhost:5341"
  },
  "Jaeger": {
    "AgentHost": "localhost",
    "AgentPort": 6831,
    "Endpoint": "http://localhost:6831",
    "Protocol": "UdpCompactThrift"
  },
  "Prometheus": {
    "ScrapeResponseCacheDurationMilliseconds": 5000
  },
  "Zipkin": {
    "Endpoint": "http://localhost:9411/api/v2/spans"
  },
  "Otlp": {
    "Endpoint": "http://localhost:4318"
  },
  "Loki": {
    "Endpoint": "http://localhost:3100"
  },
  "AspNetCoreInstrumentation": {
    "RecordException": "true"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Grafana.Loki"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://localhost:3100",
          "propertiesAsLabels": [
            "app",
            "level"
          ]
        }
      }
    ]
  }
}
